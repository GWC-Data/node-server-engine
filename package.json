{
  "name": "node-server-engine",
  "version": "1.0.0",
  "description": "Framework used to develop backend services. This package ships with a lot of features to standardize the creation of services, letting you focus on the business logic.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && cpy dist/src/**/* dist --parents && rimraf dist/src && tsc-alias -p tsconfig.build.json && npx babel src --out-dir dist --source-maps --extensions \".ts\" --ignore \"**/*.test.ts\" --copy-files --no-copy-ignored",
    "format": "prettier --write .",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test nyc --reporter=none mocha test/hooks.ts \"src/**/*.test.ts\"",
    "coverage": "nyc report --reporter=text",
    "coverage:ci": "nyc report --reporter=text-summary",
    "prepublishOnly": "npm run build",
    "test:ci": "npm run test -- --reporter spec",
    "test:compose": "docker-compose up --abort-on-container-exit --build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test && npm run coverage"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.ts": "eslint"
  },
  "keywords": [],
  "author": "Ram",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prakashmahendran/node-server-engine.git"
  },
  "bugs": {
    "url": "https://github.com/prakashmahendran/node-server-engine/issues"
  },
  "homepage": "https://github.com/prakashmahendran/node-server-engine#readme",
